s":["何はともあれおめでとう、だね♪」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["吸血姫"]},{"code":401,"indent":0,"parameters":["「いいですわね。"]},{"code":401,"indent":0,"parameters":["私（わたくし）も妹が欲しいですわ。"]},{"code":401,"indent":0,"parameters":["私からも、おめでとう。と言っておきますわ」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["竜娘"]},{"code":401,"indent":0,"parameters":["「ありがたい。"]},{"code":401,"indent":0,"parameters":["きっと元気な子が産まれるに違いない。"]},{"code":401,"indent":0,"parameters":["そこで、我からの相談なんだが……」"]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":16,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":16,"indent":null}]},{"code":213,"indent":0,"parameters":[-1,8,true]},{"code":221,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[90]},{"code":222,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「ふんふん。"]},{"code":401,"indent":0,"parameters":["産卵したら、暖めなくちゃならないから"]},{"code":401,"indent":0,"parameters":["しばらく集会にこれなくなっちゃうよと」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["竜娘"]},{"code":401,"indent":0,"parameters":["「そうなるな。"]},{"code":401,"indent":0,"parameters":["しばらくは、お主ら二人で会議してもらうことになる。"]},{"code":401,"indent":0,"parameters":["我のところで集会するのなら話は別だがな？」"]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":17,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":17,"indent":null}]},{"code":213,"indent":0,"parameters":[3,7,true]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["吸血姫"]},{"code":401,"indent":0,"parameters":["「私は　絶　対　に嫌ですわよ。"]},{"code":401,"indent":0,"parameters":["環境があまりにも悪すぎですわ。すぐ汚れるし……」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「私も尻尾が汚れるからちょっとニガテ、かなぁ……？」"]},{"code":213,"indent":0,"parameters":[-1,7,true]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["竜娘"]},{"code":401,"indent":0,"parameters":["「なんだつまらん」"]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":16,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":16,"indent":null}]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["はぁ。とため息を吐く竜娘。"]},{"code":401,"indent":0,"parameters":["吸血姫と大妖狐はあきれたような表情だ。"]},{"code":401,"indent":0,"parameters":["あまりにも住む世界が違いすぎる。"]},{"code":401,"indent":0,"parameters":["それは環境的な意味で。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["そんな時、吸血姫は手元の緑茶をゆっくり飲む。"]},{"code":401,"indent":0,"parameters":["『にがっ！』という表情をするも"]},{"code":401,"indent":0,"parameters":["声には出さない。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["吸血姫"]},{"code":401,"indent":0,"parameters":["「それで、最近妙な出来事は起こったりしてませんの？」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「そうだね……。"]},{"code":401,"indent":0,"parameters":["エルフの森でエルフ村を捜索している人間の男が二人。"]},{"code":401,"indent":0,"parameters":["街外れに住んでいるアカリちゃんが心配だね」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「でも、あの子もお家にいれば大丈夫だから"]},{"code":401,"indent":0,"parameters":["忠告しているし、多分だいじょうぶ……かな。"]},{"code":401,"indent":0,"parameters":["後はそうだね……」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「アルラウネさん達に魔物娘の調査をしている男と"]},{"code":401,"indent":0,"parameters":["伝説の野菜の種を探している男が来ただとか。"]},{"code":401,"indent":0,"parameters":["水晶が奪われる事は無かったかな。他のみんなは？」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},
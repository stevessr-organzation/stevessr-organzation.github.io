    "show": false,
    "localConfig": {
        "configFile": "data\\config.json",
        "defaultTemplate": "data\\template.trans",
        "ruby": "ruby\\bin\\ruby.exe",
        "php": "php\\php.exe",
        "addons": "www\\addons",
        "phpAddons": "www\\php\\addons",
        "defaultStagingPath": "www\/php\/cache",
        "extensions": [
            "tpp",
            "trans",
            "json"
        ],
        "iconIndex": {
            "tpp": 4,
            "trans": 3,
            "json": 3
        },
        "defaultIconIndex": 3,
        "versionHistory": "http:\/\/dreamsavior.net\/others\/verHistory.php",
        "checkVersion": "not-always",
        "defaultDocsUrl": "http:\/\/dreamsavior.net\/?post_type=docs&search_in_doc=all&s="
    },
    "scripts": {
        "test": "jest --coverage"
    }

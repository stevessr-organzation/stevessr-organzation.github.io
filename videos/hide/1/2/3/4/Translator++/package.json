        "associate-ext": "file:notes\/node-modules\/associate-ext",
        "await-fs": "^1.0.0",
        "baidu-translate-api": "^0.4.0",
        "better-copy": "^1.0.4",
        "buffer-tools": "file:..\/..\/..\/Project\/buffer-tools",
        "dragselect": "^1.13.1",
        "encoding-japanese": "^1.0.30",
        "fs-extra": "^9.0.0",
        "glob-to-regexp": "^0.4.1",
        "iconv-lite": "^0.5.1",
        "ini": "^1.3.5",
        "is-valid-path": "^0.1.1",
        "jquery": "^1.12.3",
        "js-tokens": "^6.0.0",
        "jschardet": "^3.0.0",
        "kstg": "^0.7.0",
        "light-trans": "file:..\/..\/..\/Project\/light-trans",
        "module-alias": "^2.2.2",
        "node-downloader-helper": "^1.0.18",
        "pako": "^2.0.4",
        "postman-request": "^2.88.1-postman.28",
        "promisify-child-process": "^4.1.1",
        "renpy-js": "^1.0.0",
        "request-progress": "^3.0.0",
        "rpy-parser": "file:..\/..\/..\/Project\/rpy-parser",
        "shell-escape": "^0.2.0",
        "string-argv": "^0.3.1",
        "strip-bom": "^4.0.0",
